<h2>Sign Up</h2>

<%=form_for @user do |f| %>

 <%= render partial: 'layouts/errors', locals: {object: @user} %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email%>
  <%= f.email_field :email %>

  <%= f.label :password%>
  <%= f.password_field :password %>

  <%= f.submit "Sign In" %>
<% end %>
